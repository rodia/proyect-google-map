/* ZT Latestnews-------------*/
div.latestnews > div:last-child{margin-bottom:0;}

div.moduletable div.latestnews{ margin:0 -15px ;}
div.latestnews div.latestnewsitems{
	border:1px solid #dddddd;
	background:#f6f6f6;
	width: auto !important;
	
	float:none;
}
div.latestnews div.latestnewsitems:hover{
	background:#eee;
}
div.latesnews-inner{padding:10px;overflow:hidden;}
div.latestnews div.latesnews-img {background:#fff;padding:4px;float:left;margin-right:10px;}
div.latestnews div.latestnewsitems img {border:1px solid #bfb898;padding:0;margin:0;}
div.latestnews .latestnewsdate{margin:3px 0 7px;font-size:11px;color:#babc8e;}
div.latestnews .latesnews-text{padding: 0;display:inline;font-size:12px;}
div.latestnews div.latestnewsitems h4{margin:0;font-weight:normal;font-size:16px;}

div.latestnews a.readone,
a.readmore{font-weight:bold;font-size:11px;padding-right:12px; background:url(../images/readmore.png) no-repeat right 3px;}

.read-allnews{clear:both;float:right;text-align:right;0;}
.read-allnews a.link{
	font-size:18px;
	color:#515502;text-decoration:underline;
	padding-right:25px;
	background:url(../images/arrow-big.png) no-repeat right center;
}
.read-allnews a.link:hover{text-decoration:none;color:#000;}

div.social-network h3.moduletitle{color:#201e1d;}

/* ZT Header-------------*/
.jv-innerheadline-wrap{border:none;padding:0;margin: 10px;position:relative;}
div.jv-innerheadline-wrap div.jv-proshow{border-radius:0;margin: 0 auto;}
div.jv-innerheadline-wrap .jv-proshow-display{text-align:center; left: 0 !important;}

div.jv-innerheadline-group ul li.item{ display:none;}
div.jv-innerheadline-group ul li.but_pre,
div.jv-innerheadline-group ul li.but_next{
	width:25px;
	height:23px;
	display:block;
	float:none;
	position:absolute;
	top: 50px;
	padding:0;
	z-index:99;
	background:url(../images/sellon-hand.png) no-repeat;
}
div.jv-innerheadline-group ul li.but_pre{left:4px;}
div.jv-innerheadline-group ul li.but_next{ right: 0px; background-position: right top;}


div.jv-proshow .jv-proshow-intro{display:block; background:none;}
div.jv-innerheadline-wrap .jv-proshow-intro{
	width: 250px;
	left:0;bottom:auto;
	top: 130px;
}
#memberships{margin: 18px 0;}


a.readon{font-weight:bold;font-size:11px;padding-right:12px; background:url(../images/readmore.png) no-repeat right 3px;float:right;}

/*----- ZT Maju ----*/
div.jv-majuheadline-wrapper{background:none;padding:0;border:none;}
div.jv-proshowwrap div.jv-proshow{margin:0;}
div.gkIsImage{border-radius: 5px; overflow:hidden;}
.gkIsWrapper-gk_coffe .gkIsSlide{border-radius:5px;}
