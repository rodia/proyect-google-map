#zt-highlight-inner {
    margin-top: 15px;
    text-align: right;
}
h3.label-call, h3.label-call2 {
    color: #2b67af;
    font-family: "Adobe Garamond Pro";
    font-size: 23px;
    font-weight: normal;
    margin-bottom: 5px;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
}
h3.label-call2 {
    font-size: 18px;
}
div.call-number {
    clear: both;
    color: #413E26;
    display: inline-block;
    font-family: "Adobe Garamond Pro";
    width: 100%;
}
h4.number1, h4.call-number1 {
    display: inline;
    font-size: 18px;
    font-weight: normal;
    line-height: 40px;
    margin-bottom: 0;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
}
h3.number2, h3.call-number2 {
    display: inline;
    font-size: 48px;
    line-height: 40px;
    margin-bottom: 0;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
}
h4.call-number1 {
    font-size: 14px;
}
h3.call-number2 {
    font-size: 37px;
}

/*----- ZT Pedon ----*/
div.jv-proshow{margin-bottom:25px;}
div.jv-pedonheadline-wrap .jv-proshow-intro{display:none;}
div.moduletable .jv-pedonheadline-wrap .but_pre,
div.moduletable .jv-pedonheadline-wrap .but_next{
	width:30px !important;
	height:40px;
	display:block;
	margin-right: 15px;
	background:url(../images/sellon-hand.png) no-repeat 0 center;
}
div.moduletable .jv-pedonheadline-wrap .but_next{background-position: right center; margin: 0 0 0 15px;}
div.jv-pedonheadline-wrap .jv-pedonheadline-group ul li{margin: 0 3px;}
div.jv-pedonheadline-wrap .jv-pedonheadline-group{background:none;padding:18px 0;border-top:1px solid #979868;border-bottom:1px solid #979868;}
div.jv_slideshow6_wrap .s6bar_container{background:none;padding:0;}
.jv-pedonheadline-group a:hover img,
.jv-pedonheadline-group a:active img,
.jv-pedonheadline-group a:focus img ,
.jv-pedonheadline-group ul li.active img{
	background: #797b54;
}

/* ZT Contact Form-------------*/
.htitleot{
	width:100%;
	color:#17365d;
	margin:0 0 20px 0;
	line-height:22px;
}
.htitle{
	width:100%;
	text-align:center;
	font-size:200%;
	color:#4a442a;
	padding:0 0 6px 0;
	border-bottom:1px solid #000000;
	font-weight:bold;
}
.htitle span{
	color:#17365d;
}

.hdes{
	width:100%;
	font-size:90%;
	color:#17365d;
	text-align:center;
}
#myForm{padding:15px 0;}
#myForm textarea,
#myForm input{border:none; padding:6px 10px; width: 250px;font-family:"Helvetica Neue";color:#83878a;text-transform:capitalize;font-size:14px; border:1px solid #d4d2b4; }

#myForm input.button {
  background: url("../images/button.png") repeat-x scroll 0 0 ;
  border: 0 none;
  color: #FFFFFF;
  cursor: pointer;
  float: left;
  font-size: 160%;
  font-weight: bold;
  height: 47px;
  padding: 0;
  margin: 0;
  font-family:Arial !important;
  text-align: center;
  text-shadow: none;
  text-transform: none;
  width: 275px;
}
#myForm div.zt-field{margin: 0 0 15px;overflow:hidden;}
