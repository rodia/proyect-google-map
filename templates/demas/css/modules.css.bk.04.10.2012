/* Box Color -------------*/
div.box-red{min-width: 275px; display:inline-block;float:right;}
div.box-red h3.moduletitle{
	line-height:52px;
	color:#fff;
	text-shadow:1px 1px 0 #340101;
	font-weight: normal;
	position:relative;
	z-index:9;
	font-size:20px;
	text-transform:uppercase;
	text-align:center;
	letter-spacing: 0.5;
	border-bottom: 0;
	padding:0;
	margin:0;
	background:url(../images/contact-title.jpg) repeat-x 0 0;
}

div.box-red div.modulecontent{background:#201e1d;margin-top:-3px;padding-top:5px;}

div.box-grey {background:#f6f6f6;border:1px solid #dddddd;overflow:hidden;}
div.box-grey h3.moduletitle {
	border:none;
	text-align:center;
	font-family:"Arial Narrow";
	text-transform:uppercase;
	font-weight:bold;
	margin: 10px 15px 0 ;
}

div.box-2{background:#f6f6f6;border:1px solid #dddddd;}
div.box-2 h3.moduletitle {
	border:none;
	margin: 30px 15px 0px ;
	padding:0;
}
div.box-2 div.custom{padding:0;}
div.box-2 div.latestnews{margin:0; min-height:160px;}
div.box-2 div.latestnews div.latestnewsitems{margin:0;border:none;background:none;}
div.box-2 .read-allnews{display:none;}


/* ZT Tabs -------------*/
div.wrap-number{    background:  #fff;border: 1px solid #C6C5C5;border-top:none;padding:10px;}
.jv_tab_vleft .container{background:none;}


ul.jv_tabs_title li h3{padding: 5px 10px !important;font-size:13px;font-family:arial;color:#948c51;line-height:20px;}
ul.jv_tabs_title li.active h3{color:#8a3a00;}

.vleft_panel{top:0;}
.vleft_panel_item .vleft_panel_container{
	display:block;
	padding:0 !important;
	margin-bottom:10px;
	border:2px solid #e3e4cf;
	background:#fff;
	display:inline-block;
	width:95%;
	box-shadow: 5px 5px 0 #e9e9d9;
}
.handles{margin: 10px 0;}
span.nextnav, span.prenav{width:50px ; text-indent:0;color:#948a54;font-size:11px;cursor:pointer;display:inline; font-weight:bold;}
div.jvcarousel-nav span.nextnav{
	width:28px;height:11px;
	background:url(../images/pape-arrow.png) no-repeat right bottom;
	padding-right:13px;
}
div.jvcarousel-nav span.prenav{
	height:11px;
	background:url(../images/pape-arrow.png) no-repeat left 0px;
	padding:0 10px 0 15px;
	margin-right:5px;
	border-right: 1px solid #948a54;
	font-weight:bold;
}
div.jvcarousel-nav span.prenav.no-border{border-right:none;}

h3.jvcarousel_title{margin:0 0 10px;padding-bottom:10px;border-bottom: 1px solid #e3e4cf;line-height:20px;}
h3 > .jvcarousel_mtitle{font-size: 16px; color:maroon;}
h2 > img.number-9{width:50px;height:52px;display:inline-block;top:-10px;position:relative;}

h2.headingbar{
	background:#cdccaf url(../images/headingbar.png) repeat-y right top;
	padding: 0 10px;
	height:36px;
	margin:0;
	display:block;
	font-size:20px;
	color:#fff;
	text-align:center;
}

/* ZT Case -------------*/
#zt-case{width:100%;display:inline-block;margin: 10px 0;}
#zt-case ul.list-case{margin:0;}
#zt-case ul.list-case li{background:none;padding:0;margin-bottom:2em; border-bottom: 2px solid #E8E8E8; padding-bottom:2em;}
#zt-case ul.list-case li:last-child{margin-bottom:1em;}

#zt-case ul.list-case li h3.case-pice{margin:0 0 5px 0;color:#515502;font-size:22px;font-weight:normal;}
#zt-case ul.list-case li a.case-link{margin:0;color:#babc8e;font-size:13px;text-decoration:underline;}
#zt-case ul.list-case li a.case-link:hover{color:#333; text-decoration:none;}
#zt-case p.readmore{float:right;margin:0;}
#zt-case p.readmore a{display:block; font-size:13px;}


/* Image Show-------------*/
div.moduletable .gkIsWrapper-gk_black_and_white .gkIsText{background:none;}
div.moduletable .gkIsWrapper-gk_black_and_white .gkIsSlides{-webkit-border-radius: 5px;
									-moz-border-radius: 5px;
									border-radius: 5px;}
div.moduletable .gkIsWrapper-gk_black_and_white .gkIsText h4{margin: 10px 0;padding:0;text-align:right;font-style:normal;font-family: "Georgia";}
div.moduletable .gkIsWrapper-gk_black_and_white .gkIsText h4 > a{color:#ffc200;}
div.moduletable .gkIsWrapper-gk_black_and_white .gkIsText h4 a span{color:#fff;display:block;}
div.moduletable .gkIsWrapper-gk_black_and_white .gkIsText p{margin:0;padding:5px 0;text-align:right;font-size:22px;color:#fff;border-top:1px solid #eee;font-family: "Georgia";}

